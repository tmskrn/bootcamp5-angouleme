<ul>
 <% @parkings.each do |parking| %>
  <li><%= parking.address.city if parking.address.present? %>, 
      <%= link_to('show', parking)%>, 
      <%= link_to 'edit', edit_parking_path(parking) %>, 
      <%= link_to 'delete', parking, :method => :delete, data: {confirm: 'Are you sure?'} %>
   <ul>
    <li>Places: <%= parking.places %> </li>
    <li>Day price: <%= parking.day_price %></li>
    <li>Hour price: <%= parking.hour_price %></li>
   </ul>
 </li>
 <% end %>
</ul>
