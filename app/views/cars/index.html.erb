<div>
 <ul>
  <% @cars.each do |car| %>
   <ul>
    <li><%= car.model %>, <%= car.registration_number %></li>
    <ul>
     <li><%= link_to 'show', car %></li> 
     <li><%= link_to 'edit', edit_car_path(car) %></li>
     <li><%= link_to 'delete', car, :method => :delete, data: {confirm: 'Are you sure?'} %></li>
    </ul>
   </ul>
  <% end %>
 </ul>
</div>
